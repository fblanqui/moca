(* Non trivial additive monoid.

   The code generated by Moca for this file is simple enough
   to be the first one proven in Coq :)
*)
type monoid = private
   | Zero
   | One
   | Add of monoid * monoid
     begin
       commutative
       associative
       neutral (Zero)
     end
;;
