(* $Id: monoid_list.mlm,v 1.4 2010-11-22 16:29:10 bonichon Exp $ *)

(* Monoid <'a> freely generated by type 'a *)
type 'a t = private
   | Unit
   | Gen of 'a
   | Mult of 'a t list
     begin
       associative
       neutral (Unit)
     end
;;
