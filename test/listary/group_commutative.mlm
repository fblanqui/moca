(* $Id: group_commutative.mlm,v 1.1 2008-06-13 08:47:18 weis Exp $ *)

(* Abelian group or Z-module freely generated by argument type 'a. *)

type 'a t = private
   | Zero
   | Gen of 'a
   | Opp of 'a t
   | Add of 'a t list
   begin
     associative
     commutative
     neutral left (Zero)
     neutral right (Zero)
     opposite (Opp)
   end
;;
