(* $Id: group_commutative.mlm,v 1.6 2012-06-04 13:01:22 weis Exp $ *)

(* Abelian group or Z-module freely generated by argument type 'a. *)

type 'a t = private
   | Zero
   | Gen of 'a
   | Opp of 'a t
   | Add of 'a t * 'a t
       begin
         associative
         commutative
         neutral left (Zero)
         neutral right (Zero)
         opposite (Opp)
       end
;;
