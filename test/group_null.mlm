(* $Id: group_null.mlm,v 1.6 2012-06-04 13:01:22 weis Exp $ *)

(* Minimal group structure: the group is trivial since it has no generator; 
	 hence, there is only one element in the group: the neutral element. *)

type t = private
   | Zero
   | Opp of t
   | Add of t * t
   begin
    associative
    commutative
    neutral (Zero)
    opposite (Opp)
   end
;;
