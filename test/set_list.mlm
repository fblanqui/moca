(* $Id: set_list.mlm,v 1.3 2012-06-04 13:01:22 weis Exp $ *)

(* The type of polymorphic sets.
   Repetition is not allowed. Order is irrelevant. *)

type 'a t = private
  | Empty
  | Singleton of 'a
  | Union of 'a t list
  begin
    associative commutative neutral (Empty) idempotent
  end
;;
