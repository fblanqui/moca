(* $Id: non_commutative_list.mlm,v 1.13 2012-06-04 13:01:22 weis Exp $ *)

(* Ad-hoc example for testing non commutative varyadic constructors *)

type t = private
  | Neutral
  | Nil
  | Inv of t
  | Elem of string
  | Constr1 of t list
      begin
        associative
        neutral left (Neutral)
        inverse left (Inv)
      end
  | Constr2 of t list
      begin
        idempotent right
      end
  | Constr3 of t list
      begin
        associative
        idempotent right
      end
  | Constr4 of t list
      begin
        nilpotent left (Nil)
      end
  | Constr5 of t list
      begin
        associative
        nilpotent left (Nil)
      end
  | Constr6 of t list
      begin
        neutral (Neutral)
        inverse right (Inv)
      end
;;
