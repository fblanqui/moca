(* $Id: group_list.mlm,v 1.3 2012-06-04 13:01:22 weis Exp $ *)

(* Group Gr(a) freely generated by type a *)

type 'a t = private
  | Unit
  | Gen of 'a
  | Opp of 'a t
  | Mult of 'a t list
  begin
    associative
    neutral (Unit)
    opposite (Opp)
  end
;;
