(* $Id: multiset_list.mlm,v 1.2 2008-02-11 19:37:33 weis Exp $ *)

(* The type of polymorphic finite multisets.
   Repetition is allowed. Order is irrelevant. *)

(* Remark: isomorphic to commutative monoid [a] freely generated by type a *)

type 'a t = private
  | Empty
  | Singleton of 'a
  | Union of 'a t list
  begin
    associative commutative neutral (Empty)
  end
;;
