(* The type of ordered polymorphic sets.
   Repetition is not allowed. Order is relevant. *)

type 'a t = private
  | Empty
  | Singleton of 'a
  | Plus of 'a t * 'a t
  begin
    associative neutral (Empty) idempotent
  end
;;
