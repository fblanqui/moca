(* $Id: semi_group_commutative.mlm,v 1.2 2010-11-22 16:29:10 bonichon Exp $ *)

(* Commutative semigroup freely generated by type a *)
type 'a t = private
	    | Gen of 'a
	    | Mult of 'a t * 'a t
		begin
		  associative
		  commutative
		end
;;
