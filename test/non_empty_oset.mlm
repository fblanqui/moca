(* $Id: non_empty_oset.mlm,v 1.1 2008-03-28 08:13:29 weis Exp $ *)

(* The type of ordered polymorphic sets that cannot be empty.
   Repetition is not allowed. Order is relevant. *)

type 'a t = private
  | Singleton of 'a
  | Plus of 'a t * 'a t
  begin
    associative idempotent
  end
;;
