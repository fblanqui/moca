(* $Id: semi_group_commutative_1.mlm,v 1.3 2010-11-22 16:29:10 bonichon Exp $ *)

(* Semi-group structure: just an associative commutative relation with
   only one constant generator. *)

type t = private
   | One
   | Add of t * t
   begin
     associative
     commutative
   end
;;
