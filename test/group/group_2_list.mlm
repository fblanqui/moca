(* Simple group structure: the group is not abelian
   since the operation is not commutative.
   The group has two generators. *)

type t = private
   | Two
   | One
   | Zero
   | Opp of t
   | Add of t list
   begin
     associative
     neutral (Zero)
     opposite (Opp)
   end
;;
