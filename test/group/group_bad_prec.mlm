(* $Id: group_bad_prec.mlm,v 1.1 2008-06-26 12:02:50 blanqui Exp $ *)

(* Group Gr(a) freely generated by type a *)

type 'a t = private
  | Unit
      begin
	completion precedence 10
      end
  | Gen of 'a
      begin
	completion precedence 20
      end
  | Opp of 'a t
      begin
	completion precedence 30
      end
  | Mult of 'a t * 'a t
      begin
	completion precedence 40 
	  associative
	  neutral left (Unit)
	  neutral right (Unit)
	  opposite (Opp)
      end
;;
