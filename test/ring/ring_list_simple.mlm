(* $Id: ring_list_simple.mlm,v 1.1 2010-12-20 18:02:32 weis Exp $ *)

(* Ring Z[a] freely generated by type a *)

type 'a t = private
  | Opp of 'a t
  | Mul of 'a t list
    begin
      distributive inverse (Opp)
    end
